<h1>Kittens!</h1>

<% @kittens.each do |kitten| %>
  <div>
    <p>Name: <%= kitten.name %></p>
    <p>Age: <%= kitten.age %></p>
    <p>Cuteness: <%= kitten.cuteness %></p>
    <p>Softness: <%= kitten.softness %></p>
    <p><%= link_to "View profile", kitten %></p>
  </div>
<% end %>

<%= link_to "Create a kitten", new_kitten_path %>